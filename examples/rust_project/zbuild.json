{
  "name": "rust-example",
  "version": "0.1.0",
  "compiler_flags": [],
  "linker_flags": [],
  "targets": {
    "main": {
      "name": "main",
      "type": "executable",
      "sources": ["src/main.rs"],
      "output": "rust_example",
      "dependencies": [],
      "flags": ["--edition=2021", "-C", "opt-level=2"]
